import os

def get_file_path(directory, filename):
    return os.path.join(directory, filename)

def main():
    directory_name = "data"
    file_name = "output.txt"
    
    full_path = get_file_path(directory_name, file_name)
    print(f"The generated path is: {full_path}")

if __name__ == "__main__":
    main()
