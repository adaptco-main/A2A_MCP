{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Script debugging",
            "cmakeDebugType": "script",
            "scriptPath": "${workspaceFolder}/<...>.cmake"
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Externally launched",
            "cmakeDebugType": "external",
            "pipeName": "<...>"
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Configure project",
            "cmakeDebugType": "configure",
            "clean": false,
            "configureAll": false
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Containers: Attach to Node",
            "remoteRoot": "/usr/src/app"
        },
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Script debugging",
            "cmakeDebugType": "script",
            "scriptPath": "${workspaceFolder}/<...>.cmake"
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Externally launched",
            "cmakeDebugType": "external",
            "pipeName": "<...>"
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Configure project",
            "cmakeDebugType": "configure",
            "clean": false,
            "configureAll": false
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "[PHASE_SPACE] Ghost-Void Tick (Python Geodesic)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/phase_space_tick.py",
            "console": "integratedTerminal"
        },
        {
            "name": "PowerShell Launch Script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/src/foo.ps1",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "PowerShell: Interactive Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Jurassic Pixels: Training (Python)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\jurassic_pixels\\parker_train.py",
            "args": [
                "--episodes",
                "10",
                "--export"
            ],
            "console": "integratedTerminal"
        },
        /*
        {
            "name": "Jurassic Pixels: C++ Test (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/jurassic_pixels_test.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb"
        },
        */
        {
            "name": "Jurassic Pixels: Interactive Mode (Python)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\jurassic_pixels\\parker_train.py",
            "args": [
                "--interactive"
            ],
            "console": "integratedTerminal"
        }
    ]
}