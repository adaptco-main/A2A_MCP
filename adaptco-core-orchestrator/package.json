{
  "name": "adaptco-core-orchestrator",
  "version": "0.1.0",
  "description": "Capsule registration orchestrator service for Adaptco platform.",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "build": "echo \"no build step\"",
    "test": "jest --runInBand",
    "test:accelerated": "node src/bin/rehearsal.js --duration=30m --mode=epoch_generation",
    "lint": "eslint .",
    "format": "prettier -w .",
    "start": "node src/server.js",
    "audit": "node src/bin/audit.js",
    "rehearsal": "node src/bin/rehearsal.js",
    "agent:ignite": "pnpm run rehearsal --mode=world_synthesis && pnpm run audit"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "commander": "11.1.0",
    "crypto-js": "4.2.0",
    "express": "^4.19.2",
    "pino": "8.17.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "orchestrator",
    "adaptco",
    "capsule",
    "microservice"
  ],
  "author": "Adaptco",
  "license": "MIT"
}
